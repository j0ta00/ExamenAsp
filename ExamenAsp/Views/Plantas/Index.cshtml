
@{
    ViewData["Title"] = "Index";
}
@model FryGuillermo_UI.ViewModels.VmListadoPlantas
@using FryGuillermo_UI
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>

</head>
<body>
    <form method="post" item>
        <select name="selectCategoria" multiple>
            @foreach (clsCategoria categoria in Model.ListadoCategorias())
            {
        <option value="@categoria.IdCategoria">@categoria.nombreCategoria</option>            }
        </select>
        <input type="submit" value="show details">
    </form>
</body>
</html>

